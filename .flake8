[flake8]
#GOAL: max_line_length = 79 or 99
max_line_length = 150
per-file-ignores = 
    # too many leading '#' for block comment
    */tests/*:E266
    # line too long (several test strs)
    test_granules.py:E501
    # imported but unused
    __init__.py:F401

    # GOAL:
    # syntax check doctests in docstrings
    # doctests = True